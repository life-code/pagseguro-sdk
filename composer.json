{
    "name": "life-code/pagseguro-sdk",
    "description": "PagSeguro SDK for PHP",
    "keywords": ["pagseguro", "pagseguro-sdk", "sdk", "library", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vinicius Pugliesi",
            "email": "vinicius_pugliesi@outlook.com"
        }
    ],
    "require": {
        "php": ">7.0",
        "vlucas/phpdotenv": "^2.4",
        "spatie/array-to-xml": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.2"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "PagSeguro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PagSeguro\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
